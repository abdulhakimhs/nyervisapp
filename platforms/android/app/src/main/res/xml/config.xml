<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nyervis.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="DateTimePicker">
        <param name="android-package" value="com.skwas.cordova.datetimepicker.DateTimePicker" />
    </feature>
    <name>Nyervis</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/icon/app/mdpi.png" />
    <icon density="hdpi" src="res/icon/app/hdpi.png" />
    <icon density="xhdpi" src="res/icon/app/xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
</widget>
